#include <iostream>
using  namespace std;
int main() {
    // Write C++ code here
  int arr[]={23,45,32,23,34,23,23,23,23234,4545,4,34};
  int* ptr1=arr;
  int *ptr2=&arr[0];
  int *ptr3=&arr[1];
  cout<<arr<<endl;
  cout<<&arr<<endl;
  cout<<*arr<<endl;
  cout<<*ptr1<<endl;
  cout<<ptr1<<endl;
  ptr1++;
  cout<<endl;
  cout<<*ptr1<<endl;
  cout<<ptr1<<endl;
  cout<<*ptr2<<endl;
  cout<<*ptr3<<endl;
  cout<<ptr1<<endl;
  cout<<*arr+1<<endl;
  cout<<*(arr+1)<<endl;
  cout<<arr[1]<<endl;
  cout<<1[arr]<<endl;
  cout<<*(arr+2)<<endl;
    return 0;
}
